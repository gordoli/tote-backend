{
  "name": "nestjs-tote-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run build && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npm run reset:build && node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "reset:build": "rm -rf dist && npm run build",
    "migration:create:dev": " npm run typeorm -- migration:create ./src/database/migrations/dev/$npm_config_name",
    "migration:create:prod": " npm run typeorm -- migration:create ./src/database/migrations/prod/$npm_config_name",
    "migration:generate:dev": " NODE_ENV=dev  npm run reset:build && npm run typeorm -- -d ormconfig.ts migration:generate ./src/database/migrations/dev/$npm_config_name",
    "migration:generate:prod": " NODE_ENV=prod npm run reset:build && npm run typeorm -- -d ormconfig.ts migration:generate ./src/database/migrations/prod/$npm_config_name",
    "migration:revert:dev": " NODE_ENV=dev  npm run reset:build && npm run typeorm -- -d ormconfig.ts migration:revert",
    "migration:revert:prod": " NODE_ENV=prod npm run reset:build && npm run typeorm -- -d ormconfig.ts migration:revert",
    "migration:run:prod": " NODE_ENV=prod npm run reset:build && npm run typeorm -- -d ormconfig.ts migration:run",
    "migration:run:dev": " NODE_ENV=dev  npm run reset:build && npm run typeorm -- -d ormconfig.ts migration:run",
    "seed:run:dev": " NODE_ENV=dev  npm run reset:build && npm run typeorm -- -d ormconfig.seeds.ts migration:run",
    "seed:run:prod": " NODE_ENV=prod npm run reset:build && npm run typeorm -- -d ormconfig.seeds.ts migration:run",
    "seed:create:dev": " npm run typeorm -- migration:create ./src/database/seeds/dev/$npm_config_name",
    "seed:create:prod": " npm run typeorm -- migration:create ./src/database/seeds/prod/$npm_config_name",
    "seed:revert:dev": " NODE_ENV=dev npm run reset:build && npm run typeorm -- -d ormconfig.seeds.ts migration:revert",
    "seed:revert:prod": " NODE_ENV=prod npm run reset:build && npm run typeorm -- -d ormconfig.seeds.ts migration:revert",
    "schema:sync": " npm run typeorm -- -d ./ormconfig.ts schema:sync"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.588.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@scalar/nestjs-api-reference": "^0.3.125",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "express-http-context": "^1.2.4",
    "express-session": "^1.18.0",
    "google-auth-library": "^9.10.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.18.0",
    "@types/jest": "29.5.1",
    "@types/lodash": "^4.17.4",
    "@types/multer": "^1.4.11",
    "@types/node": "18.16.12",
    "@types/nodemailer": "^6.4.15",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
